        <CommandText>SELECT     Convalidaciones.id_conv, Convalidaciones.id_estudiante, Convalidaciones.universidad, Convalidaciones.materia_conv, 
                      Convalidaciones.credito_conv, Convalidaciones.id_materia_unefa, Materia.mat_cod, Materia.materia_desc, Materia.mat_credit, 
                      Convalidaciones.fecha
